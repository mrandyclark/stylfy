<div>
	<a href="#photo_box" id="photo"><%= image_tag @outfit.outfit_photo.url(:large) %></a>
	<div id="photo_box">
	  <%= image_tag @outfit.outfit_photo.url(:fz) %>
	</div>
	<script type="text/javascript">
		new FancyZoom('photo');
	</script>	
</div>

<div>
  <b>Description:</b>
  <%=h @outfit.description %>
</div>

<div>
	<b>Style Rating:</b>
</div>



<div id="votes_<%= @outfit.id %>">
	<%= render :partial => "votes/vote", :locals => {:voteable => @outfit} %>
</div>