<% 
	unless voteable.user == current_user ||
	!logged_in? ||
	current_user.voted_on?(voteable)
%>

	<%= link_to "vote against", :action => "vote_against", :id => voteable.id %>
    | 
	<%= link_to "vote for", :action => "vote_for", :id => voteable.id %>


<% end %>

<b>Style Rating: <%= voteable.votes_for - voteable.votes_against %> (<%= voteable.votes %>)


