<% 
	unless voteable.user == current_user ||
	!logged_in? ||
	current_user.voted_on?(voteable)
%>

	<%= voteable.id %>
    <%= voteable.user_id %>
    
    <%= voteable.class %>

    <%= link_to_remote "Down",
    :url => vote_against_outfit_path(@outfit),
    :method => :post
    %>
    /
    <%= link_to_remote "Up",
    :url => vote_for_outfit_path(@outfit),
    :method => :post
    %>

<% end %>

Votes: <%= voteable.votes_for - voteable.votes_against %>


